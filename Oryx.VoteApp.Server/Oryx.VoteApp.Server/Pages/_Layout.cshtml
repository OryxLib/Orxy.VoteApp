<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 在线投票小程序管理后台</title>
     
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />

    <link href="~/css/site.css" rel="stylesheet" />
    <link href="~/css/bootstrap.theme.min.css" rel="stylesheet" />
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a asp-page="/Index" class="navbar-brand">在线投票小程序管理后台</a>
            </div>
            <div class="navbar-collapse collapse visible-lg">
                <ul class="nav navbar-nav">
                    <li><a asp-page="/">活动列表</a></li>
                    <li><a asp-page="/JDLY/Add">金典留影 录入</a></li>
                    <li><a asp-page="/JDLY/Result">金典留影 结果</a></li>
                    <li><a asp-page="/Game/CreateVoting">创建活动</a></li>
                    <li><a target="_blank" asp-page="/WxqCheck">微信墙检查</a></li>
                    <li><a target="_blank" asp-page="/Wxq">微信墙(文字)</a></li>
                    <li><a target="_blank" asp-page="/WxqPic">微信墙(图片)</a></li>
                    <li><a target="_blank" asp-page="/WxqData">微信墙数据</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="btn-group btn-group-justified navbar-fixed-bottom   visible-xs">
        <a href="/" class="btn btn-default">活动列表</a>
        <a href="/Game/CreateVoting" class="btn btn-default">创建活动</a>

        @*<a target="_blank" class="btn btn-default" href="/WxqCheck">微信墙检查</a>
            <a target="_blank" class="btn btn-default" href="/Wxq">微信墙(文字)</a>
            <a target="_blank" class="btn btn-default" href="/WxqPic">微信墙(图片)</a>*@
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
   
    <script src="https://cdn.bootcss.com/vue/2.5.13/vue.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue-resource@1.5.0"></script>
    <script src="https://unpkg.com/qiniu-js@2.1.2/dist/qiniu.min.js"></script>
    <script>
        Vue.http.options.emulateJSON = true;
        Vue.http.options.emulateHTTP = true;
    </script>
    @RenderSection("Scripts", required: false)

</body>
</html>

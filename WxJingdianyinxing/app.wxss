page {
  height: 100%;
  margin: 0 !important;
}

.container {
  margin: 0;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-end;
  padding: 100rpx 0;
  box-sizing: border-box;
  background-repeat: no-repeat;
  background-size: 101% 108%;
}

.btnImg {
  height: 120rpx;
  width: 440rpx;
  /* margin-bottom: 100rpx; */
}

button {
  width: 300rpx;
  height: 150rpx;
  font-size: 54rpx;
  background-color: transparent !important;
  color: #000;
  background-image: url(https://mioto.milbit.com/voteapp/defaultBtn.png);
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
